html,body {
    height: 100%;
    font-size: 100%;
}
body{
    overflow: hidden;
    background-image: url("../img/background.png");
    background-size: cover!important;
}/*
body:after{
    position: fixed;
    top:0;
    left:0;
    height:100%;
    width:100%;
}*/
* {
    padding: 0;
    margin: 0;
}
li {
    list-style:none;
}
span {
    display: inline-block;
}

aside {
    position:relative;
    float:left;
    width:250px;
    height:100%;
    background-color:rgba(252,252,252,0.98);
    -webkit-box-shadow: 2px 0 8px #000;
    -moz-box-shadow: 2px 0 8px #000;
    box-shadow: 2px 0 8px #000;
}

section {
    position: relative;
    height:100%;
    margin-left: 250px;
    background-color: rgba(255,255,255,.9);
}
section .background{
    position: absolute;
    height:100%;
    width: 100%;
}
section .content {
    position: absolute;
    height:100%;
    width: 100%;
    overflow: hidden;
}

.asideTop{
    position: relative;
    height: 300px;
    width: 100%;
}
.information {
    position: relative;
    padding-top: 20px;
    text-align: center;
}
.cover{
    cursor: pointer;
    position: relative;
    margin-bottom: 20px;
    margin-left: auto;
    margin-right: auto;
    width: 200px;
    height: 200px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    overflow: hidden;
    -webkit-box-shadow: 0 0 5px rgba(0,0,0,.14);
    -moz-box-shadow: 0 0 5px rgba(0,0,0,.14) ;
    box-shadow: 0 0 5px rgba(0,0,0,.14);
    background-color: rgba(0,0,0,0.3);
}
.cover div.playcontroler:nth-child(1){
    -webkit-transition:opacity .4s,background-color .4s;
    -moz-transition:opacity .4s,background-color .4s;
    -ms-transition: opacity .4s,background-color .4s;
    -o-transition: opacity .4s,background-color .4s;
    transition:opacity .4s,background-color .4s;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-box-shadow: 0 0 5px rgba(0,0,0,1);
    -moz-box-shadow: 0 0 5px rgba(0,0,0,1) ;
    box-shadow: 0 0 5px rgba(0,0,0,1);
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0);
    opacity: 0;
}
.cover.hover div.playcontroler:nth-child(1){
    opacity: 1;
    background-color: rgba(0,0,0,.6);
}
.cover.paused div.playcontroler:nth-child(1){
    opacity: 1;
    background-color: rgba(0,0,0,.7);
}
.information h3,p {
    text-shadow: 0 1px 5px rgba(0,0,0,.3);
}
.sukikirai {
    position: relative;
    text-align: center;
    margin: 20px auto 20px auto;
}
.suki{
    position: relative;
    text-align: center;
    cursor: pointer;
    width: 50px;
    height: 50px;
}
.suki img{
    -webkit-transition: transform .5s,opacity .5s;
    -moz-transition: transform .5s,opacity .5s;
    -ms-transition: transform .5s,opacity .5s;
    -o-transition: transform .5s,opacity .5s;
    transition: transform .5s,opacity .5s;
}
.suki img:nth-child(1){
    position: absolute;
    top:0;
    left:0;
    z-index: 2;
}
.suki.hover img:nth-child(1){
    opacity: 0;
}
.suki.sukidesu img:nth-child(1) {
    opacity: 0;
}
.suki.sukidesu img:nth-child(2) {
    opacity: 1;
}
.suki img:nth-child(2){
    position: relative;
    top:0;
    left:0;
    z-index: 1;
    opacity: 0;
}
.suki img:nth-child(5){
    position: absolute;
    top:0;
    left:0;
    z-index: 3;
    opacity: 0;
}
.suki.click img:nth-child(5){
    -webkit-animation: sukiclick 5s linear;
    -o-animation: sukiclick 5s linear;
    animation: sukiclick 5s linear;
}
.suki.hover img:nth-child(2){
    -webkit-transform: scale(1.1);
    -moz-transform: scale(1.1);
    -ms-transform: scale(1.1);
    -o-transform: scale(1.1);
    transform: scale(1.1);
    opacity: 1;
  }
.sukikirai .next div{
    position: relative;
    width:60px;
    height: 60px;
    top:-15px;
    margin-left:5px;
    margin-right:5px;
    cursor: pointer;
    border:solid 2px rgb(196, 196, 196);
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
}
.sukikirai .next div img{
    position: relative;
    top: 8.5px;
    width: 40px;
}
.sukikirai .next div:nth-child(2){
    -webkit-transition: transform .2s,opacity .5s;
    -moz-transition: transform .2s,opacity .5s;
    -ms-transition: transform .2s,opacity .5s;
    -o-transition: transform .2s,opacity .5s;
    transition: transform .2s,opacity .5s;
    position: absolute;
    top: -7px;
    opacity: 0;
}
.sukikirai .next.hover div:nth-child(2){
    -webkit-animation: nexthover .2s;
    -o-animation: nexthover .2s;
    animation: nexthover .2s;
}
.sukikirai .next.hoverC div:nth-child(1) {
    -webkit-animation: nexthoverC 1.2s;
    -o-animation: nexthoverC 1.2s;
    animation: nexthoverC 1.2s;
}
.sukikirai .next.click div img{
    -webkit-animation: nextclick 5s ;
    -o-animation: nextclick 5s ;
    animation: nextclick 5s ;
}
.sukikirai .next.hover div:nth-child(2){
    -webkit-animation: nexthover .8s ;
    -o-animation: nexthover .8s ;
    animation: nexthover .8s ;
}


.kirai {
    position: relative;
    width: 50px;
    height: 50px;
    cursor: pointer;
}
.kirai .mid{
    position: relative;
    width: 50px;
    height: 50px;
}
.kirai .right,.left{
    -webkit-transition: transform 1s,opacity .5s;
    -moz-transition: transform 1s,opacity .5s;
    -ms-transition: transform 1s,opacity .5s;
    -o-transition: transform 1s,opacity .5s;
    transition: transform 1s,opacity .5s;
    position: absolute;
    top: 0;
    left: 0;
    width: 50px;
    height: 50px;
}
.kirai.hover .left{
    -webkit-transform: rotateZ(-15deg) translateX(-5px);
    -moz-transform: rotateZ(-15deg) translateX(-5px);
    -ms-transform: rotateZ(-15deg) translateX(-5px);
    -o-transform: rotateZ(-15deg) translateX(-5px);
    transform: rotateZ(-15deg) translateX(-5px);
}
.kirai.hover .right{
    -webkit-transform: rotateZ(15deg) translateX(5px);
    -moz-transform: rotateZ(15deg) translateX(5px);
    -ms-transform: rotateZ(15deg) translateX(5px);
    -o-transform: rotateZ(15deg) translateX(5px);
    transform: rotateZ(15deg) translateX(5px);
}
.kirai.click .left{
    -webkit-animation: kiraiclickL 5s ;
    -o-animation: kiraiclickL 5s ;
    animation: kiraiclickL 5s ;
}
.kirai.click .right{
    -webkit-animation: kiraiclickR 5s ;
    -o-animation: kiraiclickR 5s ;
    animation: kiraiclickR 5s ;
}
.kirai.click .mid  {
    -webkit-animation: kiraiclickM 5s ;
    -o-animation: kiraiclickM 5s ;
    animation: kiraiclickM 5s ;
}
.kirai.hover .left p,.kirai.hover .right p{
    opacity: .8;
}
.kirai.hover .mid p{
    opacity: .8;
    top:-5px;
}
.suki p:nth-child(3){
    -webkit-transition:opacity .8s;
    -moz-transition:opacity .8s;
    -ms-transition: opacity .8s;
    -o-transition: opacity .8s;
    transition:opacity .8s;
    position: absolute;
    line-height: 40px;
    color:rgba(0,0,0,.6);
    top:0;
    left:5px;
    opacity: 0;
}
.suki.hover p:nth-child(3){
    opacity: 1;
    z-index: 3;
}
.suki p:nth-child(4){
    -webkit-transition:opacity .8s;
    -moz-transition:opacity .8s;
    -ms-transition: opacity .8s;
    -o-transition: opacity .8s;
    transition:opacity .8s;
    position: absolute;
    line-height: 40px;
    color:rgba(0,0,0,.6);
    top:0;
    right:5px;
    opacity: 0;
}
.suki.hover p:nth-child(4){
    opacity: 1;
    z-index: 3;
}
.kirai p{
    -webkit-transition: opacity .5s,top .5s;
    -moz-transition: opacity .5s,top .5s;
    -ms-transition: opacity .5s,top .5s;
    -o-transition: opacity .5s,top .5s;
    transition: opacity .5s,top .5s;
    color:rgba(0,0,0,.6);
    position: absolute;
    line-height: 40px;
    top:0;
    left:0;
    opacity: 0;
}
.kirai .left p{
    left: 5px;
}
.kirai .mid p{
    left: 16px;
}
.kirai .right p{
    right: -20px;
}
.list{
    position: absolute;
    bottom:0;
    width: 100%;
    margin: 20px auto 0 auto;
}
 .listtitle{
     position: relative;
    margin: 0 100px 0 10px;
    padding: 5px 50px 5px 5px;
    line-height: 100%;
   /* border-bottom: solid 2px #6cf;*/
}
.listcontent{
    position: relative;
}
.listrow {
    position: relative;
    height: 50px;
    width: auto;
    margin: 5px 20px 10px 20px;
    line-height: 50px;
    text-align: center;
    cursor: pointer;
    text-shadow: 0 0 6px rgba(0,0,0,.3);
    -webkit-box-shadow: 0 0 5px rgba(0,0,0,.4);
    -moz-box-shadow: 0 0 5px rgba(0,0,0,.4);
    box-shadow: 0 0 5px rgba(0,0,0,.4);
    -webkit-border-radius: 15px;
    -moz-border-radius: 15px;
    border-radius: 15px;
    -webkit-transition-duration: .5s;
    -moz-transition-duration: .5s;
    -ms-transition-duration: .5s;
    -o-transition-duration: .5s;
    transition-duration: .5s;
}
.listrow.click{
    -webkit-transform: scale(1.1);
    -moz-transform: scale(1.1);
    -ms-transform: scale(1.1);
    -o-transform: scale(1.1);
    transform: scale(1.1);
    opacity:0;
    box-shadow: 0 0 55px rgba(0,0,0,.0);
    z-index: 20;
    pointer-events: none;
}
.listcontent div.listrow:hover{
    -webkit-box-shadow: 0 0 5px rgba(0,0,0,.6);
    -moz-box-shadow: 0 0 5px rgba(0,0,0,.6);
    box-shadow: 0 0 5px rgba(0,0,0,.6);
}
.listcontent div.listrow div {
    height: 50px;
    width: 50px;
    line-height: 50px;
    text-align: center;
}
.progress{
    position: absolute;
    top:0;
    left: 250px;
    min-width: 64px;
    width: auto;
    font-size:24px;
    z-index:10;
}
.lrcboard{
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height:auto;
    text-align:center;
    font-size: 28px;
    padding:20px;
    text-shadow: 0 0px 6px rgba(0,0,0,.4);
